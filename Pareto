PARETO  = 
VAR TOTAL_OCORRENCIAS = CALCULATE(COUNTROWS('SSM'),ALLSELECTED(SSM))
VAR TOTAL_ATUAL = COUNTROWS('SSM')
VAR TB_RESUMO = SUMMARIZE(
    ALLSELECTED('SSM'),
    'SSM'[EQUIPAMENTO],
    "TOTAL",
    COUNTROWS('SSM')
    )
VAR TOTAL_ACUMULADO = SUMX(FILTER(TB_RESUMO,[TOTAL] >= TOTAL_ATUAL),[TOTAL])
RETURN TOTAL_ACUMULADO/TOTAL_OCORRENCIAS
